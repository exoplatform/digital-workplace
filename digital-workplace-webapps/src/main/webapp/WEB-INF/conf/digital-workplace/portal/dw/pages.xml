<?xml version="1.0" encoding="UTF-8"?>
<page-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_7 http://www.gatein.org/xml/ns/gatein_objects_1_7"
  xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_7">

  <page>
    <name>homepage</name>
    <title>Home Page</title>
    <access-permissions>*:/platform/users</access-permissions>
    <edit-permission>*:/platform/administrators</edit-permission>
    <container id="digitalWorkplaceHomePage"
      template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
      <access-permissions>*:/platform/users</access-permissions>
      <container id="SliderContainer"
        template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>presentation</application-ref>
              <portlet-ref>ContentListViewerByQueryPortlet</portlet-ref>
              <preferences>
                <preference>
                  <name>formViewTemplatePath</name>
                  <value>/exo:ecm/views/templates/content-list-viewer/list/NewsSlider.gtmpl</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>header</name>
                  <value>Slider</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>basePath</name>
                  <value>newsDetail</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>folderPath</name>
                  <value>repository:collaboration:/sites/dw</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>query</name>
                  <value>SELECT * FROM exo:news WHERE jcr:path LIKE '/Groups/spaces/communication/News/%' and exo:pinned='true' ORDER BY jcr:created DESC</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>sharedCache</name>
                  <value>false</value>
                  <read-only>false</read-only>
                </preference>
              </preferences>
            </portlet>
            <title>Slider</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
          </portlet-application>
      </container>
      <container id="MiddleContainer"
        template="system:/groovy/portal/webui/container/UIResponsiveTableColumnContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <container id="LatestNewsContainer"
          template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>presentation</application-ref>
              <portlet-ref>ContentListViewerByQueryPortlet</portlet-ref>
              <preferences>
                <preference>
                  <name>formViewTemplatePath</name>
                  <value>/exo:ecm/views/templates/content-list-viewer/list/LatestNews.gtmpl</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>header</name>
                  <value>Latest News Post</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>basePath</name>
                  <value>newsDetail</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>folderPath</name>
                  <value>repository:collaboration:/sites/dw</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>query</name>
                  <value>SELECT * FROM exo:symlink WHERE jcr:path LIKE '/Application Data/News/Pinned/%' ORDER BY jcr:created DESC</value>
                  <read-only>false</read-only>
                </preference>
                <preference>
                  <name>sharedCache</name>
                  <value>false</value>
                  <read-only>false</read-only>
                </preference>
              </preferences>
            </portlet>
            <title>News</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
          </portlet-application>
        </container>
        <container id="UserProfileContainer"
          template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <container id="ProfileStatsContainer"
            template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
            <access-permissions>*:/platform/users</access-permissions>
            <portlet-application>
              <portlet>
                <application-ref>gamification-portlets</application-ref>
                <portlet-ref>ProfileStats</portlet-ref>
              </portlet>
              <title>Profile stats</title>
              <access-permissions>*:/platform/users</access-permissions>
              <show-info-bar>false</show-info-bar>
            </portlet-application>
          </container>
          <container id="PerkstoreWalletContainer"
            template="system:/groovy/portal/webui/container/UIResponsiveTableColumnContainer.gtmpl">
            <access-permissions>*:/platform/users</access-permissions>
            <container id="PerkstoreContainer"
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>perk-store</application-ref>
                  <portlet-ref>PerkstoreOrder</portlet-ref>
                </portlet>
                <title>Perkstore Order</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
            <container id="WalletContainer"
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>wallet</application-ref>
                  <portlet-ref>WalletBalance</portlet-ref>
                </portlet>
                <title>Wallet</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
          </container>
        </container>
      </container>

      <container id="BottomContainer"
        template="system:/groovy/portal/webui/container/UIResponsiveTableColumnContainer.gtmpl">
        <access-permissions>*:/platform/users</access-permissions>
        <container id="rightbottom-spaces-container"
          template="system:/groovy/portal/webui/container/UIAddOnColumnContainer.gtmpl">
          <name>rightbottom-spaces-container</name>
          <access-permissions>*:/platform/users</access-permissions>
          <factory-id>addonContainer</factory-id>
        </container>
        <container id="TasksContainer"
          template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <portlet-application>
            <portlet>
              <application-ref>task-management</application-ref>
              <portlet-ref>tasks</portlet-ref>
            </portlet>
            <title>Tasks</title>
            <access-permissions>*:/platform/users</access-permissions>
            <show-info-bar>false</show-info-bar>
          </portlet-application>
        </container>
        <container id="DocumentsContainer"
          template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
          <access-permissions>*:/platform/users</access-permissions>
          <container
            template="system:/groovy/portal/webui/container/UIDWTabContainer.gtmpl">
            <factory-id>TabContainer</factory-id>
            <container
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <title>recentDocuments</title>
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>documents</application-ref>
                  <portlet-ref>DocumentsPortlet</portlet-ref>
                  <preferences>
                    <preference>
                      <name>appId</name>
                      <value>recentDocuments</value>
                      <read-only>false</read-only>
                    </preference>
                    <preference>
                      <name>type</name>
                      <value>recent</value>
                      <read-only>false</read-only>
                    </preference>
                  </preferences>
                </portlet>
                <title>Recent documents</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
            <container
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <title>inSpacesDocuments</title>
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>documents</application-ref>
                  <portlet-ref>DocumentsPortlet</portlet-ref>
                  <preferences>
                    <preference>
                      <name>appId</name>
                      <value>recentSpacesDocuments</value>
                      <read-only>false</read-only>
                    </preference>
                    <preference>
                      <name>type</name>
                      <value>recentSpaces</value>
                      <read-only>false</read-only>
                    </preference>
                  </preferences>
                </portlet>
                <title>Recent spaces documents</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
            <container
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <title>sharedDocuments</title>
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>documents</application-ref>
                  <portlet-ref>DocumentsPortlet</portlet-ref>
                  <preferences>
                    <preference>
                      <name>appId</name>
                      <value>sharedDocuments</value>
                      <read-only>false</read-only>
                    </preference>
                    <preference>
                      <name>type</name>
                      <value>shared</value>
                      <read-only>false</read-only>
                    </preference>
                  </preferences>
                </portlet>
                <title>Shared with me documents</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
            <container
              template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
              <title>favoriteDocuments</title>
              <access-permissions>*:/platform/users</access-permissions>
              <portlet-application>
                <portlet>
                  <application-ref>documents</application-ref>
                  <portlet-ref>DocumentsPortlet</portlet-ref>
                  <preferences>
                    <preference>
                      <name>appId</name>
                      <value>favoriteDocuments</value>
                      <read-only>false</read-only>
                    </preference>
                    <preference>
                      <name>type</name>
                      <value>favorite</value>
                      <read-only>false</read-only>
                    </preference>
                  </preferences>
                </portlet>
                <title>Favorite documents</title>
                <access-permissions>*:/platform/users</access-permissions>
                <show-info-bar>false</show-info-bar>
              </portlet-application>
            </container>
          </container>
        </container>
      </container>
    </container>
  </page>

</page-set>